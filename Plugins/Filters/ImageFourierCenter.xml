<ServerManagerConfiguration>
   <ProxyGroup name="filters">
      <SourceProxy name="ImageFourierCenter"
                   class="vtkImageFourierCenter"
                  label="Image Fourier Center">
         <Documentation
                      long_help="" 
                      short_help=""> 
         </Documentation> 

         <InputProperty
                       name="Input"
                       command="SetInputConnection">
            <ProxyGroupDomain name="groups">
               <Group name="sources"/>
               <Group name="filters"/>
            </ProxyGroupDomain>
            <DataTypeDomain name="input_type">
               <DataType value="vtkImageData"/>
            </DataTypeDomain>
         </InputProperty>

      </SourceProxy>
   </ProxyGroup> 
</ServerManagerConfiguration>
